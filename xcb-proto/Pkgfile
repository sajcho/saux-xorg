# Description:	XCB X protocol descriptions
# URL:		http://xcb.freedesktop.org
# Maintainer:	sajcho, saux dot aarch64 at gmail dot com
# Depends on:	python3

name=xcb-proto
version=1.14
release=0
source=(https://xcb.freedesktop.org/dist/${name}-${version}.tar.xz)

build() {
	cd ${name}-${version}

	./configure \
		--prefix=/usr

	make

	make DESTDIR=$PKG install
}
